{
    "version": 2.1,
    "installCommand": "pip install -r requirements.txt",
    "devCommand": "gunicorn ml_server:app",
    "functions": {
        "ml_server.py": {
            "runtime": "python"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ml_server.py"
        }
    ],
    "excludeFiles": [
        ".next/**",
        "*.cache/**",
        "node_modules/**",
        "public/**",
        "app/**",
        "static/**"
    ]
}
